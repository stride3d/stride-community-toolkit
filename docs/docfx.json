{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "Stride.CommunityToolkit/Stride.CommunityToolkit.csproj",
            "Stride.CommunityToolkit.Examples/Stride.CommunityToolkit.Examples.csproj",
          ],
          "src": "../src",
          "properties": {
            "TargetFramework": "net6.0",
            "StrideBuildDoc": "true"
          }
        }
      ],
      "dest": "api",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "flattened",
      "memberLayout": "samePage",
      "EnumSortOrder": "alphabetic",
      "allowCompilationErrors": false
    }
  ],
  "build": {
    "globalMetadata": {
      "_appTitle": "Stride Community Toolkit",
      "_enableSearch": true,
      "_appLogoPath": "media/stride-logo-blue-toolkit.svg",
      "_appFooter": "<div class=\"d-flex flex-column flex-sm-row justify-content-between pt-1 text-center small\"><p >Supported by the <a href=\"https://dotnetfoundation.org/\" target=\"_blank\" rel=\"noopener\">.NET Foundation</a></p><p>Made with <a href=\"https://dotnet.github.io/docfx\">docfx</a></p><p >Stride Docs Website v.1.0.0.0</p><p>&copy; .NET Foundation and Contributors</p></div>"
    },
    "fileMetadata": {
      "_appTitle": {
        "tutorials/**/*.md": "Stride Tutorials",
        "manual/**/*.md": "Stride Manual",
        "ReleaseNotes/**/*.md": "Stride Release Notes",
        "api/**/*.md": "Stride API"
      }
    },
    "sitemap": {
      "baseUrl": "https://doc.stride3d.net/",
      "priority": 0.1,
      "changefreq": "monthly"
    },
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md",
          "tutorials/toc.yml",
          "tutorials/**/*.md",
          "release-notes/toc.yml",
          "release-notes/**/*.md",
          "manual/toc.yml",
          "manual/**/*.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "examples/**/*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          ".nojekyll",
          "CNAME",
          "favicon.ico",
          "favicon.png",
          "images/**",
          "**/media/**/*.png",
          "**/media/**/*.jpg",
          "**/media/**/*.gif",
          "**/media/**/*.mp4",
          "**/media/**/*.zip",
          "**/media/**/*.svg",
          "**/media/**/*.webp",
          "**/media/**/*.cs"
        ],
        "exclude": [ "_site/**", "obj/**" ]
      }
    ],
    "output": "_site",
    "globalMetadataFiles": [],
    "markdownEngineProperties": {
      "toc": {
        "levels": "1,2,3"
      }
    },
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern",
      "template"
    ],
    "postProcessors": [],
    "keepFileLink": false,
    "disableGitFeatures": false
  }
}
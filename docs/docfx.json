{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "Stride.CommunityToolkit/Stride.CommunityToolkit.csproj",
            "Stride.CommunityToolkit.Bepu/Stride.CommunityToolkit.Bepu.csproj",
            "Stride.CommunityToolkit.Bullet/Stride.CommunityToolkit.Bullet.csproj",
            "Stride.CommunityToolkit.DebugShapes/Stride.CommunityToolkit.DebugShapes.csproj",
            "Stride.CommunityToolkit.ImGui/Stride.CommunityToolkit.ImGui.csproj",
            "Stride.CommunityToolkit.Skyboxes/Stride.CommunityToolkit.Skyboxes.csproj",
            "Stride.CommunityToolkit.Windows/Stride.CommunityToolkit.Windows.csproj"
          ],
          "src": "../src",
          "properties": {
            "TargetFramework": "net8.0",
            "StrideBuildDoc": "true"
          }
        }
      ],
      "dest": "api",
      "filter": "filterConfig.yml",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "flattened",
      "memberLayout": "samePage",
      "EnumSortOrder": "alphabetic",
      "allowCompilationErrors": false
    }
  ],
  "build": {
    "xref": [
      "https://doc.stride3d.net/latest/en/xrefmap.yml",
      "https://github.com/dotnet/docfx/raw/main/.xrefmap.json"
    ],
    "globalMetadata": {
      "_appTitle": "Stride Community Toolkit",
      "_enableSearch": true,
      "_appLogoPath": "media/stride-logo-blue-toolkit.svg",
      "_appFooter": "<div class=\"d-flex flex-column flex-sm-row justify-content-between pt-1 text-center small\"><p >Supported by the <a href=\"https://dotnetfoundation.org/\" target=\"_blank\" rel=\"noopener\">.NET Foundation</a></p><p>Made with <a href=\"https://dotnet.github.io/docfx\">docfx</a></p><p >Stride Community Toolkit Website v.1.0.0.6</p><p>&copy; .NET Foundation and Contributors</p></div>"
    },
    "fileMetadata": {
      "_appTitle": {
        "api/**/*.md": "Stride Community Toolkit API",
        "tutorials/**/*.md": "Stride Community Toolkit Tutorials",
        "release-notes/**/*.md": "Stride Community Toolkit Release Notes",
        "manual/**/*.md": "Stride Community Toolkit Manual",
        "contributing/**/*.md": "Stride Community Toolkit Contributing"
      }
    },
    "sitemap": {
      "baseUrl": "https://stride3d.github.io/stride-community-toolkit",
      "priority": 0.1,
      "changefreq": "monthly"
    },
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md",
          "tutorials/toc.yml",
          "tutorials/**/*.md",
          "release-notes/toc.yml",
          "release-notes/**/*.md",
          "manual/toc.yml",
          "manual/**/*.md",
          "contributing/toc.yml",
          "contributing/**/*.md"
        ]
      },
      {
        "files": [
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "examples/**/*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          ".nojekyll",
          "CNAME",
          "favicon.ico",
          "favicon.png",
          "images/**",
          "**/media/**/*.png",
          "**/media/**/*.jpg",
          "**/media/**/*.gif",
          "**/media/**/*.mp4",
          "**/media/**/*.zip",
          "**/media/**/*.svg",
          "**/media/**/*.webp",
          "**/media/**/*.cs"
        ],
        "exclude": [ "_site/**", "obj/**" ]
      }
    ],
    "output": "_site",
    "globalMetadataFiles": [],
    "markdownEngineProperties": {
      "toc": {
        "levels": "1,2,3"
      }
    },
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern",
      "template"
    ],
    "postProcessors": [],
    "keepFileLink": false,
    "disableGitFeatures": false
  }
}
